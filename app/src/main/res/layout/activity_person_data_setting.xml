<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".pojo.PersonDataSettingActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/relativeLayout31"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

    <ImageView
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:id="@+id/my_back"
        android:src="@drawable/comment_back_normal"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="10dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="我的资料"
        android:id="@+id/textView88"
        android:textSize="18sp"
        android:gravity="center"
        android:textColor="#c9000000"
        android:layout_alignTop="@+id/my_back"
        android:layout_centerHorizontal="true" />
    </RelativeLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:id="@+id/textView89"
        android:layout_below="@+id/relativeLayout31"
        android:layout_alignLeft="@+id/relativeLayout31"
        android:layout_alignStart="@+id/relativeLayout31"
        android:background="#dadada"
        />

    <RelativeLayout
        android:id="@+id/relativeLayout100"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_marginTop="50dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="头像"
            android:id="@+id/set_img"
            android:gravity="center"
            android:textColor="#000000"
            android:textSize="18sp"
            android:layout_marginTop="35dp"
            android:layout_marginLeft="20dp"
            />

        <com.pjt.xianyu.CircleImageView
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/person_img"
            android:layout_marginLeft="150dp"
            android:layout_marginTop="15dp"
            android:src="@drawable/default_user_avatar"
            />

        <ImageView
            android:layout_width="17dp"
            android:layout_height="17dp"
            android:id="@+id/imageView29"
            android:src="@drawable/entry_arrow"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginRight="20dp"
            >
        </ImageView>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout101"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginTop="150dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="昵称"
            android:id="@+id/set_text_name"
            android:gravity="center"
            android:textColor="#000000"
            android:textSize="18sp"
            android:layout_marginTop="15dp"
            android:layout_marginLeft="20dp"
            />

        <TextView
            android:id="@+id/set_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="测试名"
            android:layout_marginLeft="160dp"
            android:layout_marginTop="18dp"
            android:textSize="15dp"/>

        <ImageView
            android:layout_width="17dp"
            android:layout_height="17dp"
            android:id="@+id/imageView30"
            android:src="@drawable/entry_arrow"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginRight="20dp"
            >
        </ImageView>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout102"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginTop="210dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="性别"
            android:id="@+id/set_sex1"
            android:gravity="center"
            android:textColor="#000000"
            android:textSize="18sp"
            android:layout_marginTop="15dp"
            android:layout_marginLeft="20dp"
            />

        <TextView
            android:id="@+id/set_sex"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="男"
            android:layout_marginLeft="160dp"
            android:layout_marginTop="18dp"
            android:textSize="15dp"/>

        <ImageView
            android:layout_width="17dp"
            android:layout_height="17dp"
            android:id="@+id/imageView31"
            android:src="@drawable/entry_arrow"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginRight="20dp"
            >
        </ImageView>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout103"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginTop="270dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="电话"
            android:id="@+id/set_phone1"
            android:gravity="center"
            android:textColor="#000000"
            android:textSize="18sp"
            android:layout_marginTop="15dp"
            android:layout_marginLeft="20dp"
            />

        <TextView
            android:id="@+id/set_phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="18773603403"
            android:layout_marginLeft="160dp"
            android:layout_marginTop="18dp"
            android:textSize="15dp"/>

        <ImageView
            android:layout_width="17dp"
            android:layout_height="17dp"
            android:id="@+id/imageView32"
            android:src="@drawable/entry_arrow"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginRight="20dp"
            >
        </ImageView>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout104"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginTop="330dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="地址"
            android:id="@+id/set_address1"
            android:gravity="center"
            android:textColor="#000000"
            android:textSize="18sp"
            android:layout_marginTop="15dp"
            android:layout_marginLeft="20dp"
            />

        <TextView
            android:id="@+id/set_address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="湖南 常德"
            android:layout_marginLeft="160dp"
            android:layout_marginTop="18dp"
            android:textSize="15dp"/>

        <ImageView
            android:layout_width="17dp"
            android:layout_height="17dp"
            android:id="@+id/imageView33"
            android:src="@drawable/entry_arrow"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginRight="20dp"
            >
        </ImageView>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout105"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginTop="390dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="简介"
            android:id="@+id/set_brief1"
            android:gravity="center"
            android:textColor="#000000"
            android:textSize="18sp"
            android:layout_marginTop="15dp"
            android:layout_marginLeft="20dp"
            />

        <TextView
            android:id="@+id/set_brief"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="文理学院的一名大四学生"
            android:layout_marginLeft="160dp"
            android:layout_marginTop="18dp"
            android:textSize="15dp"/>

        <ImageView
            android:layout_width="17dp"
            android:layout_height="17dp"
            android:id="@+id/imageView34"
            android:src="@drawable/entry_arrow"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginRight="20dp"
            >
        </ImageView>

    </RelativeLayout>

</RelativeLayout>